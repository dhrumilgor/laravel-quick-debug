{
    "name": "dhr/laravel-quick-debug",
    "type": "library",
    "description": "laravel quick debug can find dd and exit and any random string from the project",
    "keywords": [ "laravel","find debug","debug","search","find code"],
    "license": "MIT",
    "authors": [
        {
            "name":     "Dhrumil Gor",
            "homepage": "https://github.com/dhrumilgor/laravel-quick-debug",
            "role":     "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Dhr\\LaravelQuickDebug\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
